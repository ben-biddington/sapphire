{
    "compilerOptions": {
      "baseUrl": ".",
      "outDir": "./dist",
      "module": "commonjs",
      "target": "es6",
      "lib": ["es6"],
      "moduleResolution": "node",
      "rootDir": ".",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noUnusedLocals": true,
    },
    "exclude": [
      "./node_modules"
    ]
  }
  